<Bindings>
	<Binding name = "LINKWRANGLERCLOSE" header = "LINKWRANGLER" category = "ADDONS">
		LinkWrangler.CloseAllWindows();
		PlaySound(PlaySoundKitID and "igMainMenuClose" or SOUNDKIT.IG_MAINMENU_CLOSE); -- HYBRID73
	</Binding>
	<Binding name = "LINKWRANGLERMINIMIZE" category = "ADDONS">LinkWrangler.MinimizeAllWindows();</Binding>
	<Binding name = "LINKWRANGLERENABLE" category = "ADDONS">LinkWrangler.MasterSwitch("toggle");</Binding>
	<Binding name = "LINKWRANGLERCAPTURE" category = "ADDONS">LinkWrangler.CaptureTooltip();</Binding>
</Bindings>
